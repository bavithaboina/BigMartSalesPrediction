   "metadata": {},
   "outputs": [],
   "source": [
    "# test data\n",
    "test[\"Item_Fat_Content\"]=test[\"Item_Fat_Content\"].replace({\"low fat\":\"Low Fat\",\"LF\":\"Low Fat\",\"reg\":\"Regular\"})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "4cf0183b",
   "metadata": {},
   "outputs": [],
   "source": [
    "train.loc[train[\"Item_Type_Combined\"]==\"Non-Consumable\",\"Item_Fat_Content\"]=\"Non-Edible\""
